{
  "name": "react-webpack",
  "version": "1.0.0",
  "main": "./src/index.tsx",
  "description": "React boilerplate with Hot Module Replacement, React 18 and Webpack 5",
  "author": "Hoang Cong Phan <hoangcp219@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "^18.12.1",
    "yarn": "1.22.19"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "storybook",
    "babel",
    "helmet",
    "HMR",
    "eslint",
    "prettier",
    "stylelint",
    "hygen",
    "Atomic design pattern",
    "boilerplate"
  ],
  "resolutions": {
    "webpack": "5"
  },
  "scripts": {
    "start": "webpack  --env NODE_ENV=prod --mode=production --config=configs/webpack/webpack.config.js",
    "dev": "webpack-dev-server --env NODE_ENV=dev --config=configs/webpack/webpack.config.js",
    "analyzer": "yarn clean && webpack --env NODE_ENV=prod --env ANALYZER=true --mode=production --config=configs/webpack/webpack.config.js && yarn stats",
    "stats": "webpack --profile --json > dist/stats.json && webpack-bundle-analyzer dist/stats.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/**/*.{ts,tsx,json}",
    "lint:fix": "eslint src/**/*.{ts,tsx,json}",
    "lint:style": "stylelint src/**/*.scss",
    "prettier": "prettier --write src/**/*.{js,ts,tsx,css,md,json} --config ./.prettierrc",
    "gen:component": "hygen components new",
    "gen:page": "hygen pages new",
    "gen:container": "hygen containers new",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist/*"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^2.9.10",
    "@reduxjs/toolkit": "^1.9.1",
    "axios": "^1.2.0",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.41.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-viewport": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/source-loader": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.5.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^9.1.0",
    "change-case": "^4.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.26.1",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "csso-webpack-plugin": "^2.0.0-beta.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^7.32.0 || ^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "hygen": "^6.2.11",
    "image-minimizer-webpack-plugin": "^3.8.1",
    "imagemin": "^8.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "json-minimizer-webpack-plugin": "^4.0.0",
    "lint-staged": "^13.0.4",
    "mini-css-extract-plugin": "^2.7.0",
    "mini-svg-data-uri": "^1.4.4",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
